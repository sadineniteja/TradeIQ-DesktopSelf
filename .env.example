# OpenAI API Keys
OPENAI_API_KEY=your_openai_api_key_here

# Model Configuration
# Model for processing trade signals
EXECUTION_MODEL=gpt-4-turbo-preview

# Model for building/updating channel prompts
BUILDER_MODEL=gpt-4-turbo-preview

# E*TRADE API Configuration
ETRADE_CONSUMER_KEY=your_etrade_consumer_key
ETRADE_CONSUMER_SECRET=your_etrade_consumer_secret
ETRADE_SANDBOX=true

# SnapTrade Configuration (if using SnapTrade proxy)
SNAPTRADE_CLIENT_ID=your_snaptrade_client_id
SNAPTRADE_CONSUMER_KEY=your_snaptrade_consumer_key
SNAPTRADE_USER_ID=your_snaptrade_user_id
SNAPTRADE_USER_SECRET=your_snaptrade_user_secret

# Discord Webhook URLs (comma-separated)
DISCORD_WEBHOOK_URLS=your_discord_webhook_url_1,your_discord_webhook_url_2

# X (Twitter) API Configuration
X_API_KEY=your_x_api_key
X_API_SECRET=your_x_api_secret
X_ACCESS_TOKEN=your_x_access_token
X_ACCESS_TOKEN_SECRET=your_x_access_token_secret
X_BEARER_TOKEN=your_x_bearer_token

# Grok API Configuration (optional)
GROK_API_KEY=your_grok_api_key

# Alpha Vantage API Configuration (optional)
ALPHAVANTAGE_API_KEY=your_alphavantage_api_key

# Server Configuration
API_PORT=5000

